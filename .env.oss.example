# wpAgentic Environment (OSS Light mode)

APP_MODE=oss
AUTH_ENABLED=false
DB_ENABLED=false

# Client-only session (no secrets stored server-side)
SESSION_SECRET=dev-only-not-used

# AI defaults
AI_DEFAULT_PROVIDER=openai
AI_DEFAULT_MODEL=gpt-4o-mini
WRITE_MODE_DEFAULT=false

# Firecrawl (optional)
FIRECRAWL_API_URL=https://your-firecrawl-host
FIRECRAWL_API_KEY=

NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=dev-only-not-used
